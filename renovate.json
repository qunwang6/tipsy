{
  "extends": [
    "config:base",
    ":semanticCommits",
    ":semanticCommitTypeAll(chore)"
  ],
  "commitMessageTopic": "{{depName}}",
  "automergeType": "branch",
  "automerge": true,
  "ignoreDeps": ["gatsby"],
  "major": {
    "automerge": false
  },
  "node": {
    "enabled": false
  },
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0
}
